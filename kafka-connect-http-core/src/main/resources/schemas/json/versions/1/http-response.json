{
  "$schema": "http://json-schema.org/draft/2019-09/schema#",
  "title": "Http Response",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "statusCode":{
      "type": "integer"
    },
    "statusMessage":  {
      "type": "string"
    },
    "protocol":  {
      "type": "string"
    },
    "responseHeaders":  {
      "type": "object",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "responseBody":  {
      "type": "string"
    }
  },
  "required": [
    "statusCode",
    "statusMessage"
  ]
}