{
  "$schema": "http://json-schema.org/draft/2019-09/schema#",
  "title": "Http Request",
  "type": "object",
  "additionalProperties": false,
  "required": ["url","method","bodyType"],
  "properties": {
    "requestId": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "string"
        }
      ]
    },
    "correlationId": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "string"
        }
      ]
    },
    "timeoutInMs": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "integer"
        }
      ]
    },
    "retries": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "integer"
        }
      ]
    },
    "retryDelayInMs": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "integer"
        }
      ]
    },
    "retryMaxDelayInMs": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "integer"
        }
      ]
    },
    "retryDelayFactor": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "number"
        }
      ]
    },
    "retryJitter": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "integer"
        }
      ]
    },
    "url": {
      "type": "string"
    },
    "headers": {
      "type": "object",
      "connect.type": "map",
      "additionalProperties": {
        "type": "array",
        "items": {
          "type": "string"
        }
      }
    },
    "method": {
      "type": "string"
    },
    "bodyAsString": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "string"
        }
      ]
    },
    "bodyAsByteArray": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "string"
        }
      ]
    },
    "bodyAsMultipart": {
      "oneOf": [
        {
          "type": "null",
          "title": "Not included"
        },
        {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      ]
    },
    "bodyType": {
      "type": "string",
      "enum": [
        "STRING",
        "BYTE_ARRAY",
        "MULTIPART"
      ]
    }
  },
  "required": [
    "url",
    "method"
  ]
}