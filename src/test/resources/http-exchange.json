{
  "$id": "https://github.com/clescot/kafka-connect-http-sink/schemas/http-exchange",
  "$schema": "http://json-schema.org/draft/2019-09/schema",
  "title": "Http Exchange",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "durationInMillis": {
      "type": "integer"
    },
    "moment": {
      "type": "number"
    },
    "attempts": {
      "type": "integer"
    },
    "success": {
      "type": "boolean"
    },
    "httpResponse": {
      "$ref": "http-response.json"
    },
    "httpRequest": {
      "$ref": "http-request.json"
    }
  },
  "required": [
    "success"
  ]

}